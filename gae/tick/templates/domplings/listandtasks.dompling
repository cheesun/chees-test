                            <div title='saveWidget' class='dialogWidget'><input type='button' value='save' title='saveButton' class='ticklistSaveButton' style='display:none'/></div>
                            <div title='shareWidget' class='dialogWidget'><input type='button' value='share' title='shareButton' class='shareButton' style='display:none'/></div>                                                            
                        </div>
                        <div class='clearing unselectable' >&nbsp;</div>                         
                    </div>
                    <div title='rootElement' class='listRootElement'>                  
                    </div>
                    <div class='clearing unselectable' >&nbsp;</div>                     
                </div>
            </div>

            <div id="template_roottask">
                <div title='task' class="roottask">
